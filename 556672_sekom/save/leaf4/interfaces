auto bridge
iface bridge
    bridge-ports mpo1 peerlink vni0001
    bridge-vids 1 10
    bridge-vlan-aware yes

auto mpo1
iface mpo1
    bond-slaves swp1
    clag-id 1

auto peerlink
iface peerlink
    bond-slaves swp31 swp32

auto peerlink.4094
iface peerlink.4094
    address 169.254.1.2/30
    clagd-backup-ip 192.168.200.2
    clagd-peer-ip 169.254.1.1
    clagd-priority 2000
    clagd-sys-mac 44:38:39:FF:01:02

auto vagrant
iface vagrant inet dhcp

auto eth0
iface eth0 inet dhcp
 vrf mgmt

auto mgmt
iface mgmt
  address 127.0.0.1/8
  vrf-table auto

auto vlan1
iface vlan1
    ip6-forward off
    ip-forward off
    vlan-id 1
    vlan-raw-device bridge

auto vni0001
iface vni0001
    bridge-access 1
    bridge-arp-nd-suppress on
    bridge-learning off
    mstpctl-bpduguard yes
    mstpctl-portbpdufilter yes
    vxlan-id 10001
    vxlan-local-tunnelip 192.168.255.104
