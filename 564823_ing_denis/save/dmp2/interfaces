auto vagrant
iface vagrant inet dhcp

auto eth0
iface eth0 inet dhcp
 vrf mgmt

auto mgmt
iface mgmt
  address 127.0.0.1/8
  vrf-table auto

auto lo
iface lo inet loopback
   address 10.30.30.2/32
   clagd-vxlan-anycast-ip 10.100.101.12


###############
# VRFs / L3VNI
###############

auto trusted
iface trusted
    vrf-table auto


auto IPCtrusted
iface IPCtrusted
    vrf-table auto


auto untrusted
iface untrusted
    vrf-table auto


auto vni404014
iface vni404014
    mtu 8950
    bridge-access 4014 
    bridge-arp-nd-suppress on
    bridge-learning off
    vxlan-id 404014
    vxlan-local-tunnelip 10.30.30.2 

